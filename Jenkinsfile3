pipeline {
    agent any
    
    stages {
        stage('Initialize') {
            steps {
                echo 'Esta es la etapa de inicialización'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Esta es la fase de construcción'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Esta es la fase de despliegue'
            }
        }
    }
}